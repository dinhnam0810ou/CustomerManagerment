<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <filter>
        <filter-name>CountFilter</filter-name>
        <filter-class>vn.evolus.training.filter.CountFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CountFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
       <servlet-name>HelloServlet</servlet-name>
       <display-name>Hello Servlet</display-name>
       <servlet-class>vn.evolus.training.servlet.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
        <url-pattern>/gidodi</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>Default.html</welcome-file>
    </welcome-file-list>
</web-app>